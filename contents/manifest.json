{

  "manifest_version": 2,
  "name": "LibDown",
  "version": "1.1",

  "description": "Quickly download books while using Amazon",
  "icons": {
    "48": "icons/index.png"
  },
  "permissions": [
    "*://libgen.rs/*",
    "*://*.amazon.com/*",
    "*://library.lol/*"
  ],
  "browser_action": {
    "default_icon": "icons/index.png",
    "default_title": "LibDown",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
  ]


}
